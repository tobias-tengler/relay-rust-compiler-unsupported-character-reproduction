{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "babel-loader": "8.1.0",
    "d3": "^7.3.0",
    "meros": "^1.1.4",
    "react": "18.0.0-rc.1",
    "react-charts": "^3.0.0-beta.35",
    "react-dom": "18.0.0-rc.1",
    "react-error-boundary": "^3.1.4",
    "react-flow-renderer": "^9.7.4",
    "react-relay": "^13.2.0",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "recoil": "^0.6.1",
    "relay-runtime": "^13.2.0",
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "relay": "relay-compiler --src ./src --schema ./src/schema.graphql --artifactDirectory ./src/__generated__",
    "schema": "curl localhost:5000/graphql?sdl -o ./src/schema.graphql",
    "format": "prettier ./src",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/d3": "^7.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-relay": "^11.0.2",
    "@types/relay-runtime": "^12.0.0",
    "@types/styled-components": "^5.1.22",
    "babel-plugin-relay": "^13.2.0",
    "customize-cra": "^1.0.0",
    "graphql": "^16.3.0",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.1.8",
    "relay-compiler": "^13.2.0",
    "relay-compiler-language-typescript": "^15.0.0",
    "relay-config": "^12.0.1",
    "typescript": "^4.1.2"
  }
}
